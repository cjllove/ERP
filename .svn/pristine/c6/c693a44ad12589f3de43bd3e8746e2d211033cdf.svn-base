                        </f:TextBox>
                        <f:TextBox ID="zsmScan" runat="server" Label="扫描本位码" LabelWidth="85px" EmptyText="扫描生产追溯码" AutoPostBack="true" OnTextChanged="zsmScan_TextChanged">
