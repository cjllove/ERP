    <script type="text/javascript">
        function PosGoodsQt(Info) {
            F('<%= hfdValue.ClientID%>').setValue(Info);
            F.customEvent('GoodsAdd');
        }

        function PrintAll() {
            var billNos = F('<%= hfdSEQNO.ClientID%>').getValue();
            if (billNos == "") {
                F.alert("请选择要打印的单据信息！");
                return;
            }
            var Report = ReportViewer.Report;
            ReportViewer.ReportURL = "/grf/rkysd.grf?timestamp=" + new Date().getTime();
            var dataurl = "/captcha/PrintReport.aspx?Method=GetStorageData&osid=" + billNos;
            ReportViewer.Report.LoadDataFromURL(dataurl);
            ReportViewer.Start();
            ReportViewer.Report.PrintPreview(true);
            ReportViewer.Stop();
        }

        function PrintYSRKB() {
            var user = F('<%= hfdUser.ClientID%>').getValue();
            var begrq = F('<%= lstLRRQ1.ClientID%>').getText();
            var endrq = F('<%= lstLRRQ2.ClientID%>').getText();
            var flag = F('<%= lstFLAG.ClientID%>').getValue();
            var gys = F('<%= lstSUPID.ClientID%>').getValue();
            var seqno = F('<%= hfdSEQNO.ClientID%>').getValue();
            var Report = ReportViewer.Report;
            ReportViewer.ReportURL = "/spdxm/xj_cjrm/grf/rksjd.grf?timestamp=" + new Date().getTime();
            var dataurl = "/spdxm/xj_cjrm/ASHX/PrintReport.aspx?Method=GetYSRKSJDData&user=" + user + "&fg=" + flag + "&gys=" + gys + "&beg=" + begrq + "&end=" + endrq + "&seqno=" + seqno;
            ReportViewer.Report.LoadDataFromURL(dataurl);
            ReportViewer.Start();
            ReportViewer.Report.PrintPreview(true);
            ReportViewer.Stop();
        }
        function btnPrint_onclick(flag) {
            var billNo = F('<%= docSEQNO.ClientID%>').getValue();
            if (billNo == "") {
                F.alert("请选择要打印的单据信息！");
                return;
            }
            var Report = ReportViewer.Report;
            ReportViewer.ReportURL = "/grf/rkysd.grf?timestamp=" + new Date().getTime();
            var dataurl = "/captcha/PrintReport.aspx?Method=GetStorageData&osid=" + billNo + "&flag=ph";
            if (flag == "PJ") {
                ReportViewer.ReportURL = "/spdxm/xj_cjrm/grf/rkysd_wxpj.grf?timestamp=" + new Date().getTime();
                dataurl = "/captcha/PrintReport.aspx?Method=GetStorageData&osid=" + billNo + "&flag=pj";
            }
            ReportViewer.Report.LoadDataFromURL(dataurl);
            ReportViewer.Start();
            ReportViewer.Report.PrintPreview(true);
            ReportViewer.Stop();
        }
        function btnPrint_BQ() {
            //高值码打印
            var billNo = F('<%= docSEQNO.ClientID%>').getValue();
            var billState = F('<%= docFLAG.ClientID%>').getValue();
            if (billNo == "") {
                F.alert("请选择要打印的单据信息！");
                return;
            }
            if (billState != "Y") {
                F.alert("选择单据未审核,不允许打印！");
                return;
            }
            ReportViewer.ReportURL = "/grf/TraceCode.grf";
            var dataurl = "/captcha/PrintReport.aspx?Method=GoodsRkGz&osid=" + billNo;
            ReportViewer.Report.LoadDataFromURL(dataurl);
            ReportViewer.Start();
            ReportViewer.Report.PrintPreview(true);
            ReportViewer.Stop();
        }
        function onTextBoxSpecialkey(box, e) {
            if (e.getKey() == e.ENTER) {
                __doPostBack('', 'TextBox1_ENTER');
            }
        }
        function onTextBoxSpd(box, e) {
            if (e.getKey() == e.ENTER) {
                __doPostBack('', 'TextBox2_ENTER');
            }
        }
        function hdfScanFun() {
            window.setTimeout(function () {
                var lightRows = F('<%= hdfScan.ClientID %>');
                var grid = F('<%= GridSacn.ClientID %>');
                $(grid.el.dom).find('.x-grid-row.highlight').removeClass('highlight');
                $.each(lightRows.getValue().split(','), function (index, item) {
                    if (item !== '') {
                        var row = grid.getView().getNode(parseInt(item, 10));
                        $(row).addClass('highlight');
                    }
                });
            }, 100);
        }

        function onOperationClick(mymsg) {
            Ext.MessageBox.show({
                msg: mymsg,
                title: '消息提示',
                buttons: Ext.Msg.YESNO,
                buttonText:
                {
                    yes: '评价',
                    no: '不评价'
                },
                fn: function (btnId) {
                    if (btnId === 'yes') {
                        __doPostBack('', 'ConfirmOK');
                    }
                }
            });
        }
        function onGridAfterEdit(event, value, params) {
            var me = this, columnId = params.columnId, rowId = params.rowId;
            if (columnId === 'BZSL') {
                var BZSL = me.getCellValue(rowId, 'BZSL');
                var BZHL = me.getCellValue(rowId, 'BZHL');
                var HSJJ = me.getCellValue(rowId, 'HSJJ');
                me.updateCellValue(rowId, 'SSSL', BZSL * BZHL);
                me.updateCellValue(rowId, 'HSJE', BZSL * HSJJ);
                var BZSLTotal = 0, HSJETotal = 0, DHSLTotal = 0;
                me.getRowEls().each(function (index, tr) {
                    BZSLTotal += parseInt(me.getCellValue(tr, 'BZSL'));
                    DHSLTotal += parseInt(me.getCellValue(tr, 'SSSL'));
                    HSJETotal += parseInt(me.getCellValue(tr, 'BZSL')) * parseInt(me.getCellValue(tr, 'HSJJ'));
                });
                me.updateSummaryCellValue('GDNAME', "本页合计", true);
                me.updateSummaryCellValue('BZSL', BZSLTotal, true);
                me.updateSummaryCellValue('HSJE', HSJETotal, true);
                me.updateSummaryCellValue('SSSL', DHSLTotal, true);
            }
        }
    </script>
</body>
