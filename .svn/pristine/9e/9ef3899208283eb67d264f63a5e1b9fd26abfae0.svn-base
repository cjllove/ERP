		Object Recordset
			ConnectionString='Provider=OraOLEDB.Oracle.1;Persist Security Info=False;User ID=dbspd;Password=dbspd;Data Source=SPD;Extended Properties=""'
			QuerySQL='SELECT A.GDSEQ,\r\n       B.GDNAME,\r\n       B.GDSPEC,\r\n       f_getunitname(B.UNIT) UNITNAME,\r\n       B.HSJJ,\r\n       SUM(A.SL) SL,\r\n       SUM(A.SL * B.HSJJ) JE,\r\n       A.PH,\r\n       A.RQ_SC,\r\n       A.YXQZ,\r\n       f_getsupname(B.PRODUCER) PRODUCERNAME,\r\n       A.HWID,\r\n       A.SUPID,\r\n       '{0}' DT,\r\n       B.HISCODE,\r\n       (CASE\r\n         WHEN A.SUPID = '00001' THEN\r\n          '初始库存'\r\n         ELSE\r\n          'SPD库存'\r\n       END) SUPNAME\r\n  FROM DAT_GOODSJXC A, DOC_GOODS B, SYS_DEPT C\r\n WHERE A.GDSEQ = B.GDSEQ(+)\r\n   AND A.DEPTID = C.CODE(+)\r\n   AND A.BILLTYPE IN ('LCD', 'CKD', 'DSC', 'LTD', 'XST')\r\n   AND A.KCADD = '1'\r\n GROUP BY A.GDSEQ,\r\n          B.GDNAME,\r\n          B.GDSPEC,\r\n          B.UNIT,\r\n          B.HSJJ,\r\n          A.PH,\r\n          A.RQ_SC,\r\n          A.YXQZ,\r\n          B.PRODUCER,\r\n          A.HWID,\r\n          A.SUPID,\r\n          B.HISCODE'
			Items Field
