@import url(/res/magnifyPic/source/jquery.fancybox.css);
@import url(/res/magnifyPic/source/helper/jquery.fancybox-buttons.css);
@import url(/res/css/layer/layer.css);

.ui-corner-all, .ui-corner-bottom, .ui-corner-right, .ui-corner-br {
    border-bottom-right-radius: 3px;
}

.ui-corner-all, .ui-corner-bottom, .ui-corner-left, .ui-corner-bl {
    border-bottom-left-radius: 3px;
}

.ui-corner-all, .ui-corner-top, .ui-corner-right, .ui-corner-tr {
    border-top-right-radius: 3px;
}

.ui-corner-all, .ui-corner-top, .ui-corner-left, .ui-corner-tl {
    border-top-left-radius: 3px;
}

.f-btn {
    padding: 4px 6px;
}

.f-grid-row-summary .f-grid-cell-inner {
    font-weight: bold;
    color: blue;
    background-color: #fff !important;
}

.f-grid-cell[data-color=color1] {
    background-color: #3AA02C;
    color: #fff;
}

.f-grid-cell[data-color=color2] {
    background-color: #BFBE24;
    color: #fff;
}

.f-grid-cell[data-color=color3] {
    background-color: #AF5553;
    color: #fff;
}

.f-grid-cell[data-color=color4] {
    background-color: #e08b14;
    color: #fff;
}

.f-grid-colheader-inner, .f-grid-cell-inner {
    padding-right: 3px;
    padding-left: 3px;
}

/* 行颜色 */
.f-grid-row[data-color=color1],
.f-grid-row[data-color=color1] .ui-icon,
.f-grid-row[data-color=color1] a {
    background-color: #3AA02C;
    color: #fff;
}

.f-grid-row[data-color=color2],
.f-grid-row[data-color=color2] .ui-icon,
.f-grid-row[data-color=color2] a {
    background-color: #BFBE24;
    color: #fff;
}
/* YMH 重新定义颜色名称 */
.f-grid-cell[data-color=spd_green] {
    background-color: #3AA02C;
    color: #fff;
}

.f-grid-cell[data-color=spd_yellow] {
    background-color: #BFBE24;
    color: #fff;
}

.f-grid-cell[data-color=spd_red] {
    background-color: #AF5553;
    color: #fff;
}

