			QuerySQL='select F_GETPARA('SUPPER') || '出库单' DT,TA.*\r\n  from (SELECT\r\n         f_getdeptname(B.DEPTOUT) DEPTOUT,\r\n         f_getdeptname(B.DEPTID) DEPTID,\r\n         B.SHRQ,\r\n         A.SEQNO,\r\n         A.GDSEQ,\r\n         A.GDNAME,\r\n         A.BARCODE,\r\n         decode(SUBSTR(A.SEQNO, 0, 2), 'DS', A.XSSL, A.BZSL) BZSL,\r\n         A.JXTAX,\r\n         A.HSJJ,\r\n         A.HSJE,\r\n         A.ZPBH,\r\n         A.HWID,\r\n         A.PZWH,\r\n         A.RQ_SC,\r\n         A.MEMO,\r\n         F_GETUNITNAME(A.UNIT) UNITNAME,\r\n         F_GETPRODUCERNAME(A.PRODUCER) PRODUCERNAME,\r\n         A.GDSPEC\r\n          FROM DAT_CK_COM A, DAT_CK_DOC B\r\n         WHERE A.SEQNO = B.SEQNO\r\n           AND A.SEQNO in ('LCD1512030010',\r\n                           'LCD1512030009',\r\n                           'DSC1512030011',\r\n                           'DSC1512030008')\r\n        UNION ALL\r\n        SELECT f_getdeptname(B.DEPTOUT) DEPTOUT,\r\n               f_getdeptname(B.DEPTID) DEPTID,\r\n               B.SHRQ,\r\n               A.SEQNO,\r\n               A.GDSEQ,\r\n               A.GDNAME,\r\n               A.BARCODE,\r\n               decode(SUBSTR(A.SEQNO, 0, 3),\r\n                      'LYD',\r\n                      A.BZSL,\r\n                      'DSC',\r\n                      (A.BZSL * A.BZHL),\r\n                      A.BZSL) BZSL,\r\n               A.JXTAX,\r\n               A.HSJJ,\r\n               A.HSJE,\r\n               A.ZPBH,\r\n               A.HWID,\r\n               A.PZWH,\r\n               A.RQ_SC,\r\n               A.MEMO,\r\n               F_GETUNITNAME(A.UNIT) UNITNAME,\r\n               F_GETPRODUCERNAME(A.PRODUCER) PRODUCERNAME,\r\n               A.GDSPEC\r\n          FROM DAT_SL_COM A, DAT_SL_DOC B\r\n         WHERE A.SEQNO = B.SEQNO\r\n           AND A.SEQNO in ('LCD1512030010',\r\n                           'LCD1512030009',\r\n                           'DSC1512030011',\r\n                           'DSC1512030008'))