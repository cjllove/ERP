			ConnectionString='Provider=OraOLEDB.Oracle.1;Persist Security Info=False;User ID=dbspd3;Password=dbspd3;Data Source=SPD_CJ;Extended Properties=""'
			QuerySQL='SELECT S.SUPID,\r\n       S.SUPNAME,\r\n       NVL(KF.RKSL, 0) RKSL,\r\n       NVL(KF.RKJE, 0) RKJE,\r\n       NVL(ABS(SY.SYSL), 0) SYSL,\r\n       NVL(ABS(SY.SYJE), 0) SYJE,\r\n       '2016年3月医疗物资供应商汇总表' DT,\r\n       0 FPS,\r\n       0 FPJE,\r\n       0 WFPJE,\r\n       '' MEMO\r\n  FROM (SELECT * FROM DOC_SUPPLIER WHERE ISSUPPLIER = 'Y') S,\r\n       (SELECT J.PSSID,\r\n               SUM(DECODE(J.BILLTYPE, 'RKD', J.SL, 'THD', J.SL, 0)) RKSL,\r\n               SUM(DECODE(J.BILLTYPE, 'RKD', J.HSJE, 'THD', J.HSJE, 0)) RKJE\r\n          FROM DAT_GOODSJXC J\r\n         WHERE J.DEPTID IN (SELECT CODE\r\n                              FROM SYS_DEPT\r\n                             WHERE TYPE IN ('1', '2')\r\n                               AND ISLAST = 'Y')\r\n         GROUP BY J.PSSID) KF,\r\n       (SELECT J.PSSID,\r\n               SUM(DECODE(J.BILLTYPE,\r\n                          'XSD',\r\n                          J.SL,\r\n                          'XSG',\r\n                          J.SL,\r\n                          'DSH',\r\n                          J.SL,\r\n                          'SYD',\r\n                          J.SL,\r\n                          'XST',\r\n                          DECODE(J.KCADD, '1', J.SL, 0),\r\n                          0)) SYSL,\r\n               SUM(DECODE(J.BILLTYPE,\r\n                          'XSD',\r\n                          J.SL * J.HSJJ,\r\n                          'XSG',\r\n                          J.SL * J.HSJJ,\r\n                          'DSH',\r\n                          J.SL * J.HSJJ,\r\n                          'SYD',\r\n                          J.SL * J.HSJJ,\r\n                          'XST',\r\n                          DECODE(J.KCADD, '1', J.SL * J.HSJJ, 0),\r\n                          0)) SYJE\r\n          FROM DAT_GOODSJXC J\r\n         WHERE J.DEPTID IN (SELECT CODE\r\n                              FROM SYS_DEPT\r\n                             WHERE TYPE IN ('3', '4')\r\n                               AND ISLAST = 'Y')\r\n         GROUP BY J.PSSID) SY'
